{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "gemini-cli": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@choplin/mcp-gemini-cli",
        "--allow-npx"
      ],
      "env": {}
    }
  }
}